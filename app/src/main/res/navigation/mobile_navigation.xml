<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_piece">

    <fragment
        android:id="@+id/navigation_piece"
        android:name="com.example.mobileversion.ui.piece.PiecesFragment"
        android:label="@string/title_piece"
        tools:layout="@layout/fragment_pieces" />

    <fragment
        android:id="@+id/navigation_userList"
        android:name="com.example.mobileversion.ui.userlist.UserListFragment"
        android:label="@string/title_userList"
        tools:layout="@layout/fragment_userlist" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.mobileversion.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>